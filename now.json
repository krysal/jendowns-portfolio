{
  "version": 2,
  "alias": [ "jendowns.com" ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "status": 301,
      "headers": { "Location": "https://jendowns.com/$1" }
    },
    { "src": "/.*", "status": 404, "dest": "404.html" }
  ]
}
